{
  ".platform-linux atom-text-editor, .platform-win32 atom-text-editor": {
    "alt-ctrl-b": "atom-k9:run",
    "alt-ctrl-shift-w": "atom-k9:watch",
    "alt-ctrl-c": "atom-k9:close"
