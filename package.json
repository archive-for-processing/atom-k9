{
  "name": "atom-k9",
  "main": "./lib/atom-k9",
  "version": "1.5.0",
  "description": "Watch JRubyArt sketches in Atom",
  "keywords": [
    "jruby_art",
    "processing",
    "ruby",
    "watch"
  ],
  "configSchema": {
    "atom-k9-executable": {
      "type": "string",
      "description": "The path to the k9 executable. Find by running `which k9`",
      "default": "k9"
    }
  },
  "activationCommands": {
    "atom-workspace": "atom-k9:run"
  },
  "repository": "https://github.com/ruby-processing/atom-k9",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3"
  }
}
